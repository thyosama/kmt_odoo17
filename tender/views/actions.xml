<odoo>

  <record model="ir.actions.act_window" id="action_window_project_draft">
        <field name="name">Draft Projects</field>
        <field name="res_model">project.project</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{'default_type':'draft'}</field>
        <field name="domain">[('type','=','draft')]</field>
<!--        <field name="view_ids" eval="[(5, 0, 0),-->
<!--                (0, 0, {'view_mode': 'tree', 'view_id': ref('tender.view_construction_project_tree')}),-->
<!--                (0, 0, {'view_mode': 'form', 'view_id': ref('tender.view_project_form')})]"/>-->
    </record>
    <record model="ir.actions.act_window" id="action_window_project">
        <field name="name">Projects</field>
        <field name="res_model">project.project</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{'default_type':'project'}</field>
        <field name="domain">[('type','=','project')]</field>
<!--        <field name="view_ids" eval="[(5, 0, 0),-->
<!--                (0, 0, {'view_mode': 'tree', 'view_id': ref('tender.view_construction_project_tree')}),-->
<!--                (0, 0, {'view_mode': 'form', 'view_id': ref('tender.view_project_form')})]"/>-->
    </record>
    <record model="ir.actions.act_window" id="action_window_item_product">
        <field name="name">Items</field>
        <field name="res_model">product.item</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record model="ir.actions.act_window" id="action_job_cost_template">
        <field name="name">Break Down Template</field>
        <field name="res_model">construction.job.cost</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{'default_is_template':True}</field>
        <field name="domain">[('is_template','=','True')]</field>
        <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('master_data_construction.view_job_cost_tree_2')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('master_data_construction.view_job_cost_fromm')})]"/>
    </record>
     <record id="tender_account_config" model="ir.actions.act_window">
            <field name="name">Settings</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">res.config.settings</field>
            <field name="view_mode">form</field>
            <field name="target">inline</field>
            <field name="context">{'module' : 'tender', 'bin_size': False}</field>
        </record>

    <record model="ir.actions.act_window" id="action_window_all_products">
        <field name="name">Product</field>
        <field name="res_model">product.template</field>
        <field name="view_mode">kanban,tree,form</field>
    </record>
    <record model="ir.actions.act_window" id="action_job_related">
        <field name="name">Related Job</field>
        <field name="res_model">tender.related.job</field>
        <field name="view_mode">tree,form</field>
    </record>
 <record model="ir.actions.act_window" id="action_upload_tender">
        <field name="name">Upload Tender</field>
        <field name="res_model">construction.tender</field>
        <field name="view_mode">tree,form</field>
     <field name="domain">[('state','=','main')]</field>
    </record>
 <record model="ir.actions.act_window" id="action_top_sheet">
        <field name="name">Top Sheet </field>
        <field name="res_model">top.sheet</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record model="ir.actions.act_window" id="action_indirect_cost">
        <field name="name">Indirect</field>
        <field name="res_model">indirect.cost</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record model="ir.actions.act_window" id="action_offers">
        <field name="name">Offers</field>
        <field name="res_model">construction.sale.order</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[('active','in',(True,False))]</field>

    </record>


    <record id="action_crm_project" model="ir.actions.act_window">
        <field name="name">New Project</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">project.project</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="project.edit_project"/>
        <field name="target">new</field>
    </record>








</odoo>