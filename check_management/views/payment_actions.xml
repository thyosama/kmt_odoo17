<odoo>
    <record id="view_account_payment_cheque_pop_wizard" model="ir.ui.view">
    <field name="name">view_account_payment_cheque_pop_wizard</field>
    <field name="model">account.payment.wizard</field>

    <field name="arch" type="xml">
        <form>
            <sheet>
                <group>
                    <field name="state_cheque2" invisible="1"/>
                    <field name="is_transfer" invisible="1"/>
<!--                    <field name="is_vendor_payments" invisible="1" />-->
                    <field name="is_pay_cash" invisible="1"/>

                    <field name="journal_under_collection"
                           invisible="is_pay_cash == True or is_transfer == True or state_cheque2 != 'under_collect'" required="state_cheque2 == 'under_collect'"/>
                    <field name="date_under_collection"
                           invisible="is_pay_cash == True or is_transfer == True or state_cheque2 != 'under_collect'" required="state_cheque2 == 'under_collect'"/>
                    <field name="journal_collection"
                           invisible="is_pay_cash == True or is_transfer == True or state_cheque2 != 'reconciled'" required="state_cheque2 == 'reconciled'"/>
                    <field name="date_collection"
                           invisible="is_pay_cash == True or is_transfer == True or state_cheque2 != 'reconciled'" required="state_cheque2 == 'reconciled'"/>
                      <field name="journal_transfer"
                             invisible="is_pay_cash == True or is_transfer == False" required="is_transfer == True"/>
                    <field name="transfer_date"
                           invisible="is_pay_cash == True or is_transfer == False" required="is_transfer == True"/>
                    <field name="journal_cash"
                           invisible="is_pay_cash == False or is_transfer == True" required="is_pay_cash == True"/>
                    <field name="date_cash"
                           invisible="is_pay_cash == False or is_transfer == True" required="is_pay_cash == True"/>
                </group>
                <footer>

                    <button name="save_payment_multi" class="oe_highlight" string="save" type="object" />
                    or
                    <button string="Cancel" class="oe_link" special="cancel" />
                </footer>
            </sheet>
        </form>
    </field>
    </record>
