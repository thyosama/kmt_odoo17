<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- QWeb Reports -->
        <record id="action_report_subcontractor" model="ir.actions.report">
            <field name="name">SubContractor</field>
            <field name="model">construction.engineer</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">new_construction.report_subcontractor</field>
            <field name="report_file">new_construction.report_subcontractor</field>
            <field name="binding_model_id" ref="model_construction_engineer"/>
            <field name="binding_type">report</field>
        </record>
        <record id="action_report_owner_contractors" model="ir.actions.report">
            <field name="name">Owner Contractors</field>
            <field name="model">construction.contract</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">new_construction.report_owner_contractors</field>
            <field name="report_file">new_construction.report_owner_contractors</field>
            <field name="binding_model_id" ref="model_construction_contract"/>
            <field name="binding_type">report</field>
        </record>
        <record id="action_report_owner_contractors" model="ir.actions.report">
            <field name="name">مذكرة التجاوز</field>
            <field name="model">construction.engineer</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">new_construction.report_construction_engineer</field>
            <field name="report_file">new_construction.report_construction_engineer</field>
            <field name="binding_model_id" ref="model_construction_engineer"/>
            <field name="binding_type">report</field>
        </record>

        <record id="paperformat_subcontractor_report" model="report.paperformat">
            <field name="name"> Subcontractor Report</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">20</field>
            <field name="margin_bottom">15</field>
            <field name="margin_left">5</field>
            <field name="margin_right">5</field>
            <field name="header_spacing">15</field>
            <field name="dpi">90</field>
        </record>

        <record id="new_construction.action_report_subcontractor" model="ir.actions.report">
            <field name="paperformat_id" ref="new_construction.paperformat_subcontractor_report"/>
        </record>
        <record id="new_construction.action_report_owner_contractors" model="ir.actions.report">
            <field name="paperformat_id" ref="new_construction.paperformat_subcontractor_report"/>
        </record>

    </data>
</odoo>
