<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <template id="thank_you_template" name="Thank You Page">
        <t t-call="website.layout">
            <div id="wrap" class="oe_structure oe_empty">
                <section class="s_text_block pt40 pb40 o_colored_level"
                    data-snippet="s_text_block" data-name="Text"
                    style="background-image: none;">
                    <div class="s_allow_columns container">
                        <div class="row">
                            <div class="col-lg-12 col-xl-11 me-lg-auto o_colored_level">
                                <span
                                    class="fa fa-thumbs-up bg-primary fa-4x mx-auto d-block img-thumbnail rounded rounded-circle o_anim_fade_in o_animate o_animate_both_scroll"
                                    data-bs-original-title="" title=""
                                    aria-describedby="tooltip303890"
                                    style="animation-duration: 2s !important;" />
                                <br />
                                <h1 class="text-center">Good Job</h1>
                                <div class="pb16 pt16 s_hr" data-snippet="s_hr"
                                    data-name="Separator">
                                    <hr class="mx-auto border-top w-50 border-dark text-center" />
                                </div>
                                <h5 class="text-center">
                                    <span class="fa fa-check-circle" />
                                    <span>Your Timesheet has been sent <b>successfully</b>
                                    </span>
                                </h5>
                                <p class="text-center">Thank You!</p>
                            </div>

                        </div>
                    </div>
                </section>
            </div>
        </t>
    </template>
    
</odoo>