<odoo>
    <record id="view_cost_analysis_pop_wizard" model="ir.ui.view">
        <field name="name">view_cost_analysis_pop_wizard</field>
        <field name="model">cost.analysis.wizard</field>

        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>

                        <field name="project_ids"/>
                    </group>
                    <footer>
                    <button name="get_cost_analysis" string="View" type="object" class="btn-primary"/>

                    <button name="cancel" string="Cancel" type="object" class="btn-primary"/>


                </footer>
                </sheet>


            </form>
        </field>
    </record>

</odoo>