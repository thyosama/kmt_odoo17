<odoo>

    <record id="view_time_variance_pop_wizard" model="ir.ui.view">
        <field name="name">view_time_variance_pop_wizard</field>
        <field name="model">time.variance</field>

        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>

                        <field name="project_ids" widget="many2many_tags"/>
                    </group>
                    <footer>
                    <button name="view_report" string="View" type="object" class="btn-primary"/>

                    <button name="cancel" string="Cancel" type="object" class="btn-primary"/>


                </footer>
                </sheet>


            </form>
        </field>
    </record>
</odoo>