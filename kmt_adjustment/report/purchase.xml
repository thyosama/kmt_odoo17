<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="kmt_report_purchasequotation_document" inherit_id="purchase.report_purchasequotation_document">
            <xpath expr="//span[contains(@t-field, 'o.name')]/.." position="after">
                <strong t-if="o.po_agreement">Po Agreement:
                    <span t-field="o.po_agreement"/>
                </strong>
            </xpath>
        </template>
        <template id="kmt_report_purchaseorder_document" inherit_id="purchase.report_purchaseorder_document">
            <xpath expr="//span[contains(@t-field, 'o.name')]/.." position="after">
                <strong t-if="o.po_agreement">Po Agreement:
                    <span t-field="o.po_agreement"/>
                </strong>
            </xpath>
        </template>
    </data>
</odoo>